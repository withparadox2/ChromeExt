{
  "name": "ChromeExt",
  "version": "1.00",
  "manifest_version": 2,
  "description": "Clean way for surfing net.",
  "browser_action": {
    "default_icon": "icon128.png"
  },
  "icons": {
    "128": "icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.nytimes.com/*",
        "*://*.archive.org/*",
        "*://*.theatlantic.com/*",
        "*://*.vk.com/*",
        "*://*.spotify.com/*",
        "*://*.wekan.tv/*",
        "*://*.youtube.com/*",
        "*://*.youglish.com/*",
        "*://*.feedly.com/*",
        "file://*",
        "*://podcasts.google.com/*"
      ],
      "js": ["./content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["webRequest", "webRequestBlocking", "tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  }
}
